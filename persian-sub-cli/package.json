{
  "name": "persian-sub-cli",
  "version": "0.1.1",
  "description": "Preparing Persian Subtitles: CLI",
  "license": "MIT",
  "author": {
    "name": "Nasser Rafie",
    "email": "contact@geminorum.ir",
    "url": "https://geminorum.ir"
  },
  "homepage": "https://github.com/brothersincode/persian-sub#readme",
  "bugs": {
    "url": "https://github.com/brothersincode/persian-sub/issues"
  },
  "files": [
    "index.js",
    "persian-sub.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brothersincode/persian-sub"
  },
  "bin": {
    "persian-sub": "./persian-sub.js"
  },
  "keywords": [
    "persian",
    "srt",
    "subtitle",
    "virastar"
  ],
  "dependencies": {
    "commander": "^2.14.1",
    "extend": "^3.0.1",
    "persian-sub-encoding": "^0.1.1",
    "persian-sub-rename": "^0.1.1",
    "persian-sub-virastar": "^0.1.1",
    "through2": "^2.0.3",
    "vinyl-fs": "^3.0.2"
  },
  "devDependencies": {
    "iconv-lite": "^0.4.19",
    "is-utf8": "^0.2.1",
    "jschardet": "^1.6.0",
    "virastar": "^0.13.0"
  },
  "nexe": {
    "input": "./persian-sub.js",
    "output": "persian-sub^$",
    "temp": ".nexe",
    "runtime": {
      "framework": "node",
      "ignoreFlags": true
    }
  }
}
